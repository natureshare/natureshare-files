  <entry>
    <id>https://files.natureshare.org/psoralea/items/ns/2011/53b39850e35eb1298400e744.yaml</id>
    <title>Eucalyptus pauciflora</title>
    <updated>2011-05-13T22:53:18.000+00:00</updated>
    <published>2011-05-13T22:13:07.000+00:00</published>
